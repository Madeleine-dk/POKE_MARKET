<div class="navbar navbar-expand-sm navbar-light navbar-poke">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">
      <%= image_tag "Pokemarket" %>
    </a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ms-auto">
        <li class="nav-item">
        <%= link_to new_user_session_path do %>
          <button class="btn btn-login me-2" type="button"><strong>Login</strong></button>
        <% end %>
        </li>
        <li class="nav-item">
        <%= link_to new_user_registration_path do %>
          <button class="btn btn-signin" type="button"><strong>Sign Up</strong></button>
         <% end %>
        </li>
      </ul>
    </div>
  </div>
</div>
